{
    "manifest_version": 3,
    "name": "Trolley",
    "description": "we do a little trolling",
    "version": "1.0",
    "icons": {
        "16": "files/trl_16.png",
        "32": "files/trl_32.png",
        "48": "files/trl_48.png",
        "128": "files/trl_128.png"

    },
    "permissions":[
        "storage"
    ],
    "content_scripts":[{
            "matches": ["<all_urls>"],
            "js": ["script.js"]
        }],
    "web_accessible_resources": [{
        "matches": ["<all_urls>"],
        "resources": ["files/troll_laugh.png",
        "files/trollface.png",
        "files/trolley.png"]
    }]



}